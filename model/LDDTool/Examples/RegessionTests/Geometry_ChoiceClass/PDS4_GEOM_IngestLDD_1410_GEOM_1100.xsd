    </xs:simpleContent>
  </xs:complexType>

  <xs:simpleType name="spacecraft_to_target_subspacecraft_distance_WO_Units">
    <xs:restriction base="pds:ASCII_Real">
	   </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="spacecraft_to_target_subspacecraft_distance">
